<div>
  <% if @task.image.present? %>
    <%= image_tag(@task.image.url) %>
    <%= link_to 'Delete image', list_task_delete_image_path(@task.list, @task), method: :delete %>
  <% end %>
  <%= @task.title %> 
  <%= @task.notes %> 
  <%= @task.start_date %> 
  <%= @task.due_date %> 
  <%= @task.status %> 
   
  <%= link_to 'Edit', edit_list_task_path(@task.list_id, @task) %> 
</div>
